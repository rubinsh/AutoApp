<div ng-controller="ModelVideosCtrl" >
  <div class="col-xs-12 text-right" ng-show="videos.length" ng-cloak>
    <div ng-repeat="video in videos" class="row car-list-item">
      <iframe title="YouTube video player" class="youtube-player" type="text/html"
        width="100%" max-width="450" height="300" ng-src="{{trustUrl(video.videoUrl)}}"
        frameborder="0" allowFullScreen></iframe>
      <div class="col-xs-12 text-right">
        <h4>{{video.title}}</h4>
      </div>
    </div>
  </div>
  <div ng-if="videos.length === 0"><h4>{{defaultVideoText}}</h4></div>
</div>
